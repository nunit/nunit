    <Content Include="..\framework\framework-features.wxi">
      <Link>framework-features.wxi</Link>
    </Content>
    <Content Include="..\framework\framework-files.wxi">
      <Link>framework-files.wxi</Link>
    </Content>
    <Content Include="..\runners\addin-files.wxi">
